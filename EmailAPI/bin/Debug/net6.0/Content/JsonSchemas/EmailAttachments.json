{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "EmailAttachment": {
      "type": "object",
      "properties": {
        "ID": {
          "type": "string",
          "format": "uuid"
        },
        "FilePath": {
          "type": "string"
        },
        "FileName": {
          "type": "string",
          "maxLength": 50
        },
        "EmailId": {
          "type": "string",
          "format": "uuid"
        }
      },
      "required": [ "ID", "FilePath", "FileName", "EmailId" ]
    }
  }
}


  
