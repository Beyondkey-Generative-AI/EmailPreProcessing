{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "ID": {
        "type": "string",
        "format": "uuid"
      },
      "OrgName": {
        "type": "string",
        "maxLength": 50
      },
      "Address": {
        "type": "string",
        "maxLength": 100
      },
      "Date_Created": {
        "type": "string",
        "format": "date-time"
      },
      "Date_Modified": {
        "type": "string",
        "format": "date-time"
      }
    },
    "required": ["OrgName", "Address", "Date_Created", "Date_Modified"]
  }
  